<% provide(:title, 'Vendors') %>

<% (0..@results.length-1).each do |i|  %>
    <%= @results[i]['name'] %><%=@results[i]['count'] %>
<% end %>

  <h1>VENDORS</h1>

  <div class="filter-block" id="category-block">
    <%= render 'shops/category_filter' %>
  </div>

  <div class="container-fluid">
    <div class="span8">

      <div class="transparent-s">x</div> 
    
      <div id ="tag-div" class="center">
       <%= render 'shops/style_tags' %>
      </div>
      
      <div class="transparent-s">x</div> 
      <div class="transparent-s">x</div>

    
      
      <div id="shop-feed">
      <h3><%= @c.capitalize %></h3>  
        <%= render @shops %>
      </div>
    </div>
  </div>
